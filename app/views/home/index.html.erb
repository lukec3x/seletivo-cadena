<% if director_signed_in? %> 
 <div> Welcome <%= current_director.email %> </div> 
  <%= button_to "Sign out Director", destroy_director_session_path, method: :delete %> 
<% else %>
  <%= button_to "Sign in Director", new_director_session_path %> 
<% end %>

